{
  "name": "ryantoddgarza",
  "description": "portfolio site",
  "version": "1.0.0",
  "private": true,
  "author": "Ryan Todd Garza <ryantoddgarza@gmail.com>",
  "homepage": "https://ryantoddgarza.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryantoddgarza/ryantoddgarza.git"
  },
  "bugs": {
    "url": "https://github.com/ryantoddgarza/ryantoddgarza/issues"
  },
  "scripts": {
    "bootstrap": "npm run hooks:install && npm install",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "npm run format:code && npm run format:other",
    "format:code": "npm run lint:code -- --fix",
    "format:other": "npm run lint:other -- --write",
    "hooks:install": "git config core.hooksPath ./scripts/git",
    "hooks:uninstall": "git config --unset core.hooksPath",
    "lint": "npm run lint:code && npm run lint:other",
    "lint:code": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:other": "prettier \"**/*.md\" --check",
    "serve": "gatsby serve",
    "start": "npm run develop"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "@types/react-dom": "^17.0.9",
    "clipboard": "^2.0.6",
    "dotenv": "^10.0.0",
    "gatsby": "^3.14.6",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-catch-links": "^3.14.0",
    "gatsby-plugin-google-gtag": "^3.14.0",
    "gatsby-plugin-image": "^1.14.2",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-remove-trailing-slashes": "^3.14.0",
    "gatsby-plugin-robots-txt": "^1.7.0",
    "gatsby-plugin-sass": "^4.14.0",
    "gatsby-plugin-sharp": "^3.14.3",
    "gatsby-plugin-sitemap": "^4.10.0",
    "gatsby-remark-copy-linked-files": "^4.11.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^5.11.0",
    "gatsby-remark-prismjs": "^5.11.0",
    "gatsby-remark-responsive-iframe": "^4.11.0",
    "gatsby-remark-smartypants": "^4.11.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-json": "^3.14.0",
    "gatsby-transformer-remark": "^4.11.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "html-loader": "^2.1.2",
    "markdown-loader": "^6.0.0",
    "prismjs": "^1.22.0",
    "react": "^16.14.0",
    "react-collapsible": "^2.8.4",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "react-truncate": "^2.4.0",
    "react-twitter-widgets": "^1.9.5",
    "sass": "^1.34.0"
  },
  "devDependencies": {
    "@bitpas/eslint-config": "^0.6.1",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.29.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.3.0"
  },
  "browserslist": [
    "defaults"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "*.md": "prettier --write"
  }
}
