{
  "name": "ryantoddgarza",
  "description": "portfolio site",
  "version": "1.0.0",
  "private": true,
  "author": "Ryan Todd Garza <ryantoddgarza@gmail.com>",
  "homepage": "https://ryantoddgarza.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryantoddgarza/ryantoddgarza.git"
  },
  "bugs": {
    "url": "https://github.com/ryantoddgarza/ryantoddgarza/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "deploy": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "npm run format:code && npm run format:other",
    "format:code": "npm run lint:code -- --fix",
    "format:other": "npm run prettier -- --write",
    "lint": "npm run lint:code && npm run lint:other",
    "lint:code": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:other": "prettier \"**/*.md\" --check",
    "serve": "gatsby serve",
    "start": "npm run develop"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "clipboard": "^2.0.6",
    "gatsby": "^2.31.1",
    "gatsby-image": "^2.7.0",
    "gatsby-plugin-catch-links": "^2.7.0",
    "gatsby-plugin-google-gtag": "^2.6.0",
    "gatsby-plugin-manifest": "^2.4.34",
    "gatsby-plugin-offline": "^3.2.31",
    "gatsby-plugin-react-helmet": "^3.9.0",
    "gatsby-plugin-remove-trailing-slashes": "^2.7.0",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sharp": "^2.10.1",
    "gatsby-plugin-sitemap": "^2.9.0",
    "gatsby-plugin-styled-components": "^3.9.0",
    "gatsby-remark-copy-linked-files": "^2.3.19",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.3.33",
    "gatsby-remark-prismjs": "^3.5.16",
    "gatsby-remark-responsive-iframe": "^2.4.17",
    "gatsby-remark-smartypants": "^2.3.13",
    "gatsby-source-filesystem": "^2.3.34",
    "gatsby-transformer-json": "^2.8.0",
    "gatsby-transformer-remark": "^2.8.39",
    "gatsby-transformer-sharp": "^2.8.0",
    "html-loader": "^1.3.2",
    "markdown-loader": "^6.0.0",
    "nanoid": "^3.1.16",
    "prismjs": "^1.22.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "react-toggle": "^4.1.1",
    "react-truncate": "^2.4.0",
    "react-twitter-widgets": "^1.9.5",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.3.0"
  },
  "browserslist": [
    "> 10%",
    "IE >= 9",
    "last 2 versions"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "*.md": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
